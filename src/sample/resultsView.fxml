<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="600.0" prefWidth="1600.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ResultsViewController">
   <children>
      <LineChart fx:id="firstMethodLineChart" layoutX="14.0" layoutY="14.0" prefHeight="500.0" prefWidth="500.0">
        <xAxis>
          <NumberAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </LineChart>
      <LineChart fx:id="secondMethodLineChart" layoutX="575.0" layoutY="14.0" prefHeight="500.0" prefWidth="500.0">
         <xAxis>
            <NumberAxis side="BOTTOM" />
         </xAxis>
         <yAxis>
            <NumberAxis side="LEFT" />
         </yAxis>
      </LineChart>
      <LineChart fx:id="thirdMethodLineChart" layoutX="1086.0" layoutY="14.0" prefHeight="500.0" prefWidth="500.0">
         <xAxis>
            <NumberAxis side="BOTTOM" />
         </xAxis>
         <yAxis>
            <NumberAxis side="LEFT" />
         </yAxis>
      </LineChart>
      <HBox alignment="CENTER" layoutX="1326.0" layoutY="539.0" prefHeight="46.0" prefWidth="260.0" spacing="30.0">
         <children>
            <Button mnemonicParsing="false" onAction="#saveResultsOnAction" text="Zapisz wyniki" />
            <Button mnemonicParsing="false" onAction="#closeTheWindowOnAction" text="Zamknij okno" />
         </children>
      </HBox>
   </children>
</AnchorPane>
